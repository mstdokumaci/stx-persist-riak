{
  "name": "stx-persist-riak",
  "version": "0.0.1",
  "description": "riak kv persistence plugin for stx",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "cover": "istanbul cover --report none --print detail test",
    "dev": "nodemon --watch src --watch test -x 'npm run build && npm test'",
    "pretest": "npm run build",
    "test": "node test/index.js | tap-difflet && standard",
    "travis": "npm run build && npm run cover -s && istanbul report lcov && (cat coverage/lcov.info | coveralls) && standard",
    "publish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mstdokumaci/stx-persist-riak"
  },
  "author": "Mustafa Dokumacı <mustafa@foraa.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mstdokumaci/stx-persist-riak/issues"
  },
  "homepage": "https://mstdokumaci.github.io/stx-persist-riak",
  "keywords": [
    "stx",
    "persist",
    "riak",
    "kv"
  ],
  "contributors": [
    "Mustafa Dokumacı <mustafa@foraa.com>"
  ],
  "dependencies": {
    "basho-riak-client": "^2.0.0"
  },
  "devDependencies": {
    "stx": "latest",
    "coveralls": "^3.0.2",
    "istanbul": "0.4.5",
    "nodemon": "^1.18.7",
    "rollup": "0.67.3",
    "rollup-plugin-buble": "0.19.4",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.4.0",
    "standard": "^12.0.0",
    "tap-difflet": "0.7.1",
    "tape": "^4.9.1"
  }
}
